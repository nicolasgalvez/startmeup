{% if menu %}
  <ul class="flex">
  {% for item in items %}
    <li class="px-2 {{ item.classes | join(' ') }}">
      <a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
      {% include "partials/menu.twig" with {'items': item.children} %}
    </li>
  {% endfor %}
  </ul>
{% endif %}
